//test div-by-zero
int main () {
    int x = 7;
    int y = 5;
    int z = 4;
    int a1 = x / z; // 1
    int a2 = x % z; // 3
    int b1 = - x / - - z; // -1
    int b2 = - x % - - z; // -3
    int c1 = - x / - z; // 1
    int c2 = - x % - z; // - 3
    int d1 = x / - z; //  -1
    int d2 = x % - z; // 3
    return y / (a2 - d2);
}
