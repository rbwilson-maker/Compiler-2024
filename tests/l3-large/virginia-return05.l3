// simple for loop with arguments to functions

int fun2(int n);

int fun1(int n1, int n2, int n3, int n4, int n5, int n6, int n7, int n8, int n9, int n10)
{
	return n1+n2+n3+n4+n5+n6+n7+n8+n9+n10;

}

int main () {
  int j = 0;
  for(int i = 0; i < 10; i++)
  	j += fun1(i,i+1,i+2,i+3,i+4,i+5,i+6,i+7,i+8,i+9);
  return j;

}