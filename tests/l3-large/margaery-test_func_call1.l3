int sum(int x, int y) {
    return x + y;
}

int mul(int x, int y) {
    return x * y;
}

int div(int x, int y) {
    return x / y;
}

int sub(int x, int y) {
    return x - y;
}

int mod(int x, int y) {
    return x % y;
}

int main() {
    int x = 3;
    int y = 4;

    int z = sub(div(mul(x, y), sum(x, y)), mod(mul(x, y), sum(x, y)));

    return z;
}

