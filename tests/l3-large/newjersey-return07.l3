int fib(int n);

int main(){
  return fib(11);
}

int fib(int n)
{
  assert (n>=0);
  if(n == 0)
    return 1;
  if(n == 1)
  {
    return 1;
  }
  else
  {
    return (fib(n-1) + fib(n-2));
  }
}                                                                                                           
