//test return 27
//squareRoot returns the floor of the squre root of input
//as the given input is 783, the result should be 27

int squareRoot(int x){
  if (x == 0 || x == 1){
    return x;
  }
  int root = 1;
  while (root*root <= x) {
    root++;
  }
  return root-1;
}

int main() {
  int x = 783;
  return squareRoot(x);
}