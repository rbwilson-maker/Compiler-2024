int search(int n) {
    if (n % 5 == 0)
        return n;
    else {
        if (n > 2)
            search (n - 2);
    }
    return 0;
}
int main(){
    int x = 20;
    int y = 2;
    if(y == 0)
        return x / y;
    while(x != 0){
        x--;
    }
    int res = search(x);
    return res;
}