// calcualte the 30th fibonacci number recursively (30 is reasonably large without memoization)

int fib(int i);

int main() {
  return fib(30);
}

int fib(int i) {
  if (i == 0) {
    return 0;
  } else if (i == 1) {
    return 1;
  } else {
    return fib(i-1) + fib(i-2);
  }
}
