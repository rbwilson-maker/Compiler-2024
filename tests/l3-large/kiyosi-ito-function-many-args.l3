int telescope(int one, int two, int three, int four, int five, int six, int seven, int eight) {
  int t1 = one - two;
  int t2 = two - three;
  int t3 = three - four;
  int t4 = four - five;
  int t5 = five - six;
  int t6 = six - seven;
  int t7 = seven - eight;
  return t1 + t2 + t3 + t4 + t5 + t6 + t7;
}

