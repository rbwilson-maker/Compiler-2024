// Bitwise binary operators

int main() {
    int x = 1;
    x = x + 5;  // 6
    x = x - 3;  // 3
    x = x * 2;  // 6
    x = x / 2;  // 3

    x = x % 3;  // 0

    if (x < -2 || x <= -2) {
        x = x + 5;       // Won't happen
    }

    if (x != 5) {
        x = 17;         // 17
    }

    if (x > 18 && x != 23) {
        x = x + 5;      // Won't happen
    }

    if (x == 17) {
        x += 3;         // 20
    }

    x = x << 2;         // 80

    x = x >> 3;         // 10

    return x;
}
