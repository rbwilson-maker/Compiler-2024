// loop with if

int main() {

    int a = 2;
    int b = 5;

    for(int i = 0; i < 10; i++) {
        if(i % 2 == 0) {
            for(int j = 5; j >= 0; j--) {
                a += 3;
        }
        }
        else {
            for(int k = 0; k < 7; k++) {
                b += 2;
        }
        }        
    }

    return 2*a - 3*b;

}