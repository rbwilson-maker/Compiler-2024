// test operations
int main(){
    int a = 10;
    for(int i =10; i < 100; i++){
        a++;
        if((i%10) == 0){
            a>>1;
        }else if((i%10) == 1){
            a<<1;
        }else if((i%10) == 2){
            a^= 90;
        }else if((i%10) == 3){
            a &= 10;
        }else if((i%10) == 4){
            a >>= 2;
        }
        
    }
    return a;
}