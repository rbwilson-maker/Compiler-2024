//test return 10000

int main() {
    int sum = 0;

    for (int a = 0; a < 10; a++) {
        for (int b = 0; b < 10; b++) {
            int x = 0;
            while (x < 10) {
                for (int c = 0; c < 10; c++) {
                    sum += 1;
                }
                x += 1;
            }
        }
    }

    return sum;
}
