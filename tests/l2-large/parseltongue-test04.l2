//test return 10

int main()
{
    int a = 10;
    int b = 10;
    int c = 10;
    int d = 0;

    while (a > 0)
    {
        while (b > 0)
        {
            while (c > 0) 
            {
                d += 1;
                c -= 1;
            }
            b -= 1;
        }
        a -= 1;
    }
    return d;
}