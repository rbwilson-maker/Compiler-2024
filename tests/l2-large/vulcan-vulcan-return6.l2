int main() {
    int result = 1;
    bool found = false;
    int count = 0;

    while (!found) {

     if (result % 7 == 0) {
    	if (result % 5 == 0) {
	   if (result % 3 == 0) {
	   	if (result % 2 == 0) {
		    while (result % 11 != 0) {
		    	result ++;
		    }
		    count ++;
		    if (count >= 3) {
		    	found = true;
		    }
		}
	    }
	}
      }
      result++;
    }

    return result;
}
