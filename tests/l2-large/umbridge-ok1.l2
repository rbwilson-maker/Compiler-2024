int main()
{
    // identifiers are concats of keywords
    int forwhile = 2147483647; // INT_MAX
    int ifelse = forwhile + 1; // overflow to INT_MIN
    int returntrue = 0;
    while (forwhile > 0)
    {
        returntrue += 1;
        forwhile >>= 1;
    }
    return returntrue;
    return ifelse;
}
