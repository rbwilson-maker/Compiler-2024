//test div-by-zero
//division by 0 error triggered by int_min div -1

int main()
{
  int TMAX;
  int wrong;
  int a;

  a = -1;
  TMAX = 2147483647;

  wrong = (TMAX - a) / a;

  int x;
  int y;
  int z;

  x = 0;
  y = 1;
  x++;
  x *= 10;
  z = y << x;
  
  if(z > 10000){
    return wrong;
  }
  else {
    return wrong;
  }

}