// test if statements and boolean operations
int main() {
  bool b1 = 4 > 5;		//false
  bool b2 = 9 > 1;		//true
  bool b3 = 5 >= 5;		//true
  bool b4 = !b3;		//false
  bool b5 = b1 != b2;	//true
  bool b6 = b1 ? false : true;	//true
  if (b1)
    return 1;
  else if (b2) {
    if (b3 && b4)
	  return 3;
	else
	  if (b5 || b6)
	    return 4;
	  else
	    return 5;
  } else
    return 2;
}
