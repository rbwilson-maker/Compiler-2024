//test return -775242806
// bitwise operations
int main() {
  int a = 0x2EB54435;				//00101110101101010100010000110101
  int b = 0xCDA8AB13;				//11001101101010001010101100010011
  int c = 0xEE6342B1;				//11101110011000110100001010110001
  int aANDb = a & b;				//00001100101000000000000000010001
  int aANDbNOT = ~aANDb;			//11110011010111111111111111101110
  int aANDbNOTORc = aANDbNOT | c;	//11111111011111111111111111111111
  int res = aANDbNOTORc ^ a;		//11010001110010101011101111001010
  return res;
}
