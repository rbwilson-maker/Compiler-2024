//test return 11

int main() {
    int i;
    int n;
    int a;
    int b;
    int c;

    n = 10;
    a = 0;
    b = 0;
    c = 0;

    for (i = n; i >= 0; i--) {
        if (n % 3 == 0 && n % 2 == 0) {
            a++;
        }

        if (n % 3 == 0) {
            b++;
        }

        if (n % 2 == 0) {
            c++;
        }
    }

    return a + b + c;
}
