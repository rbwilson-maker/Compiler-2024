// test all binary operators for int

int main() {
  int x = 1;
  int y = 2;
  int z = 0;
  
  if (x + y != 3) return 1;
  if (x - y != -1) return 1;
  if (x * y != 2) return 1;
  if (x / y != 0) return 1;
  if (x % y != 1) return 1;
  
  if ((x & y) != 0) return 1;
  if ((x | y) != 3) return 1;
  if ((x ^ y) != 3) return 1;
  
  if ((x << y) != 4) return 1;
  if ((y << x) != 4) return 1;
  if ((x >> y) != 0) return 1;
  if ((y >> x) != 1) return 1;
  
  return 0;
}
