int main() {
    int x = 0x798ECD3F;
    int y = 0;
    while(x > 1) {
        y++;
        if(x % 2 == 0) {
            x >>= 1;
        } else {
            x *= 3;
            x++;
        }
    }
    while(y > 1) {
        if(y % 2 == 0) {
            y /= 2;
        } else {
            y *= 3;
            y+=1;
        }
    }
    return y;
}
