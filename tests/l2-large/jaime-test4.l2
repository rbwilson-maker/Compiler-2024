int main()
{
    int x = 19;
    int y = 23;
    int temp = x;
    int answer1 = 0;
    int answer2 = 0;
    while(temp > 0)
    {
        answer1 = answer1 + y;
        temp = temp - 1;
    }
    while(x > 0)
    {
        int y2 = y;
        while(y2 > 0)
        {
            answer2 += 1;
            y2 = y2 - 1;
        }
        x = x - 1;
    }
    return answer1 + answer2;
}
