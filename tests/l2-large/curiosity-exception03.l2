//test div-by-zero
//division by 0 error triggered by int_min mod -1

int main()
{
  int TMAX;
  int wrong;
  int a;

  a = -1;
  TMAX = 2147483647;

  wrong = (TMAX - a) % a;

  int x;
  int y;
  int z;
  y = 1;
  z = 10000;

  for(x=0; x < z; x++){
    y += x;
  }

  while (z > 0) {
    y -= 1;
    z --;
  }

  return wrong;
}
