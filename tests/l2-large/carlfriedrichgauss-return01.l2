//test return 14
// play with all operators
int main() {
    int a = 6;
    int b = 3;
    int c;

    // Using basic arithmetic operators
    c = a + b; // 9
    c = c - 1; // 8
    c = c * 2; // 16
    c = c / 2; // 8
    c = c % 7; // 1

    // Using binary arithmetic operators on c
    c += 5; // 6
    c -= 2; // 4
    c *= 3; // 12
    c /= 2; // 6
    c %= 5; // 1

    // Combining with bitwise operators for completeness (not purely arithmetic)
    c &= 3; // 1 (01 & 11 = 01)
    c |= 2; // 3 (01 | 10 = 11)
    c ^= 1; // 2 (11 ^ 01 = 10)
    c <<= 1; // 4 (10 << 1 = 100)
    c >>= 1; // 2 (100 >> 1 = 10)

    // Final operations
    c = c + 1 * 2 / 1 - 1; // 3
    c += 3 * 3 + 2 / 1; // 14

    return c;
}
