// Simple while loop with if then else statement program
int main() {
    int a = 5;
    int w = 0;
    int result = 0;
 
    for(int i = 0; i < 4; i++) {
        int temp = i+1;
        while(i < temp) {
            result += 3;
            i += 1;
        }
        i -= 1;
    }

    return result;
}
