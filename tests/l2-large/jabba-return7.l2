//test return 7

int main() {
    bool x = true;
    int z = 3;
    int q = 1;
    while (x) {
        int y = z * 2;
        if (y > z) {
            return 7;
        } else {
            y--;
        }
        q = 2;
        while(q < z) {
            q++;
            z--;
        }
        if (q > (y / 2)) {
            x = false;
        }
    }
    return q*2 + 1;
}