//test return 5
// valid usage of all control structures
int main() {
    int sum = 0;
    for (int i = 0; i <= 4; i++) {
        if (i % 2 == 0) {
            sum += i;
        }
        else {
            while (i < 2) {
                i++;
                sum += 1;
            }
        }
    }
    return sum;
}
