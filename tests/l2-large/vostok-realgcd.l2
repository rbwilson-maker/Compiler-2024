// an actual calculation for gcd(36, 48)

int main() {
    int a = 36;
    int b = 48;

    while(true) {
        if (a == 0)
           return b;
        if (b == 0)
           return a;

        // base case
        if (a == b)
            return a;

        // a is greater
        if (a > b)
            a = a - b;
        else
            b = b - a;
    }
    return 1;
}