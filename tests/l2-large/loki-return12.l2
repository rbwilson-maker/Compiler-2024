int main() {
    int n;
    int i;
    int a;
    int b;
    int c;

    n = 10;
    a = 0;
    b = 0;
    c = 0;

    i = n;
    while (i >= 0) {
        if (i % 3 == 0 && i % 2 == 0) {
            a++;
        }

        if (i % 3 == 0) {
            b++;
        }

        if (i % 2 == 0) {
            c++;
        }
        i--;
    }

    return a + b + c;
}
