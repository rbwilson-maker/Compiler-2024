//test return 6667

int main() {
    int result = 0;

    while (result < 1000) {
    	if (result % 5 == 0) {
	   result++;
	   result *= 5;
	   result += 2;
	}
	if (result % 5 == 1) {
	   result += 3;
	}

    	if (result % 5 == 2) {
	   result *= 10;
	   result -= 4;
	}

    	if (result % 5 == 3) {
	   result *= 10;
	   result -= 5;
	}

    	if (result % 5 == 4) {
	   result *= 5;
	   result ++;
	}

	result++;
    }

    return result;
}
