//test return 1860
int main()
{
    int i;
    int j;
    int k;
    int sum = 0;
    for (i = 0; i < 10; i++)
    {
        sum += i;
        j = i;
        while (j < 10)
        {
            sum += j;
            for (k = j; k < 10; k++)
            {
                sum += k;
            }
            j++;
        }
    }

    return sum;
}
