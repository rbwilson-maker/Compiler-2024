int main() {
  int a = 1;
  int b = 2;
  int c = 3;
  int d = 4;
  int val = 0;
  while (a < b) {
    while (a < c) {
      while (a < d) {
        for (int i = 0; i < 2; i++) {
          if (a < c) {
            val += (a+b+c);
          }
          else {
            val *= (a+b+c+d);
          }
          a += 1;
        }
      }
    }
  }
  return val;
}
