// Nested conditions and loops

int main() {
  int x = 0;
  int y = 1;

  if (x == 0) {
    if (x == 1) {
      return 1;
    } else {
      for(int i = 1; i < 5; i++) {
        int j = i;
        while (j > 0) {
          y *= j;
          j--;
        }
      }
    }
  } else {
    return 2;
  }

  return y;
}
