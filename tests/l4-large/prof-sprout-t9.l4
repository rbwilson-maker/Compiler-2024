typedef struct node node;

struct node {
  int data;
  struct node *next;
};

void bubble_sort(node *list, int len) {
  if (list == NULL) return;
  node *curr;
  node *last = NULL;
  bool again = true;
  while(again) {
    again = false;
    curr = list;
    while (curr->next != last) {
      if(curr->data > curr->next->data) {
        int temp = curr->data;
        curr->data = curr->next->data;
        curr->next->data = temp;
        again = true;
      }
      curr = curr->next;
    }
    last = curr;
  }
}

void print_list(node *l) {
	while(l != NULL) {
    print_int(l->data);
    l = l->next;
  }
