int matadd(int[] A, int[] B, int n) {
    int i = 0;
    int sum = 0;
    for (i = 0; i < n; i++) {
        sum += A[i] + B[i];
    }
	return sum;
}

int main() {
    int[] A;
    int[] B;
    int n = 5;

    A = alloc_array(int, 5);
    B = alloc_array(int, 5);
    int i = 0;
    for (i = 0; i < n; i++) {
        A[i] = 10;
        B[i] = 10;
    }

    return matadd(A, B, n);
}
