int sum(int[] arr, int size) {
    int total = 0;
    for (int i = 0; i < size; i++) {
        total += arr[i];
    }
    return total;
}

int main() {
    int size = 5;
    int[] arr = alloc_array(int, size);
    arr[0] = 0;
    arr[1] = 3;
    arr[2] = 4;
    arr[3] = 1;
    arr[4] = 2;
    return sum(arr, size);
}
