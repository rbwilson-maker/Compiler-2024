int main(){
    int[] arr1 = alloc_array(int, 10);
    int expected = 0;
    int result = 0; 

    for (int i = 0; i < 10; i++){
        expected+=i;
        arr1[i] = i;
    }


    for (int i = 0; i < 10; i++){
        result += arr1[i];
    }

    assert(result == expected);
    return 0;
}










