// Struct array

struct long {
    int upper;
    int lower;
};

typedef struct long long;

int main() {
    long[] long_array = alloc_array(long, 4);
    long_array[0].upper = 1;
    long_array[0].lower = 2;
    long_array[1].upper = 3;
    long_array[1].lower = 4;
    long_array[2].upper = 5;
    long_array[2].lower = 6;
    long_array[3].upper = 7;
    long_array[3].lower = 8;
    return long_array[3].lower * long_array[2].upper;
}