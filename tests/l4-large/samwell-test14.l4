void modifyarr(int[] arr, int length){
  for(int i = 0; i < length; i++){
    if (arr[i] == 0){
      arr[i]++;
    }
    else{
      arr[i]--;
    }
  }
  return;
}

int main() {
  int length = 10;
  int[] arr = alloc_array(int, length);
  modifyarr(arr, length);
  return arr[0];
 }
