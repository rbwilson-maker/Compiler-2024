int main() {
    int[] A;
    int[] B;

    A = alloc_array(int, 10);
    B = alloc_array(int, 10);

    int i = 0;

    for (i = 0; i < 10; i++) {
        A[i] = i * 1;
        B[i] = i * 2;
    }
    int val = 0;
    for (i = 0; i < 10; i++) {
        val += A[i] + B[i];
    }

    return val;
}
