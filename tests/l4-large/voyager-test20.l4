int det(int[][] mat)
{
    return mat[0][0] * mat[1][1] - mat[1][0] * mat[0][1];
}

int main()
{
    int[][] mat = alloc_array(int[], 2);
    mat[0] = alloc_array(int, 2);
    mat[1] = alloc_array(int, 2);
    mat[0][0] = 5;
    mat[0][1] = 3;
    mat[1][0] = 2;
    mat[1][1] = 2;
    return det(mat);
}