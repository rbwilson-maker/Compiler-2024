int sum(int[] nums, int len)
{
	int result = 0;
	for (int i = 0; i < len; i++) {
		result += nums[i];
	}
	return result;
}

int main()
{
	int[] nums = alloc_array(int, 10);
	nums[0] = 1;
	nums[1] = 2;
	nums[2] = 3;
	nums[3] = 4;
	nums[4] = 5;
	nums[5] = 6;
	nums[6] = 7;
	nums[7] = 8;
	nums[8] = 9;
	nums[9] = 10;
	return sum(nums, 10);
}